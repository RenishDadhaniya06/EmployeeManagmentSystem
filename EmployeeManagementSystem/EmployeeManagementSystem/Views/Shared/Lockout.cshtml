<link href="~/Libs/Gentelella/css/custom.min.css" rel="stylesheet" />
<link href="~/Content/common.css" rel="stylesheet" />
<body class="login">
    <div>
  
        <a class="hiddenanchor" id="signin"></a>
        <div class="login_wrapper">
