@model IEnumerable<EmployeeMangmentSystem.Repository.Models.ViewModel.DisplayInterviewerModel>


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />

    <style type="text/css">

        .tftable {
            width: 100%;
            border-width: 1px;
            border-collapse: collapse;
        }

            .tftable th {
                font-size: 15px;
                font-weight: bold;
                background-color: #acc8cc;
                border-width: 1px;
                padding: 8px;
                border-style: solid;
                text-align: left;
            }
    </style>
    <title>EmployeeS</title>
</head>
<body>

    <table class="tftable" border="1" style="width: 100%;border-style:solid; border-width:1px;" cellpadding="4" cellspacing="4">

        <tbody>
            <tr>
                @*<th>&nbsp;Id</th>*@
                <th>&nbsp;Employee</th>
                <th>&nbsp;Designation</th>
                <th>&nbsp;Technology</th>
                <th>&nbsp;Time Duration</th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    @*<td>&nbsp;@item.Id</td>*@
                    <td>&nbsp;@item.Employee</td>
                    <td>&nbsp;@item.Designation</td>
                    <td>&nbsp;@item.Technology</td>
                    <td>
                        &nbsp;
                        @{
                            string temp = "Avaliable From " + item.FromTime + " To " + item.ToTime;
                        }
                        @temp
                    </td>
                </tr>
            }

        </tbody>
    </table>
</body>
</html>
