@{
    ViewBag.Title = "Dashboard";
}
<h2>Appointment Calendar</h2>
<div class="scheduler_container">
    @Html.Raw(Model.Render())
</div>
<style>
    body {
        padding-top: 50px;
        padding-bottom: 20px;
    }

    .scheduler_container {
        width: 100%;
        height: 600px;
        border: 1px solid #aeaeae;
    }
</style>
<script type="text/javascript">
      toastr.options = {
        "closeButton": true,
        "debug": false,
        "newestOnTop": true,
        "progressBar": true,
        "positionClass": "toast-bottom-right",
        "preventDuplicates": true,
        "onclick": null,
        "showDuration": "3000",
        "hideDuration": "1000",
        timeOut: 0,
    extendedTimeOut: 0,
    tapToDismiss: true,
        "showEasing": "swing",
        "hideEasing": "linear",
        "showMethod": "fadeIn",
        "hideMethod": 'fadeOut'
    }

    var msgUrl = '@Url.Action("GetMessages", "Notification")';
        $.ajax({
        url: msgUrl,
        type: 'get',
        success: function (data) {
            debugger
            if (data != null) {
                debugger
                console.log("Successfull"),
                    console.log(data)
                for (var i = 0; i < data.length; i++) {
                       toastr.info(data[i].Message);
                }
            }
        },
    });
</script>

